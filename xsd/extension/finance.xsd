    	<xs:annotation>
    		<xs:documentation>A data concept that indicates whether the Federal account symbol is a Direct Loan Financing Account, a Guaranteed Loan Financing Account, or is not a financing account as defined by the Federal Credit Reform Act of 1990. Reference: SF 133, P and F</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="FundTypeCode" nillable="true" type="fin:FundTypeCodeType">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents a classification established in law that describes an OMB account's relationship to the Government, and the source of the receipts that the account is provided. Reference: FAST Book</xs:documentation>
    	</xs:annotation>    
    </xs:element>
    
    <xs:element name="NormalBalanceCode" nillable="true" type="fin:NormalBalanceCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept representing the normal condition of the balance in an USSGL account (debit or credit). Reference: SF 133, FMS 2108, P and F</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="PriorYearAdjustmentCode" nillable="true" type="fin:PriorYearAdjustmentCodeType">
       <xs:annotation>
    		<xs:documentation>A data concept that represents changes to obligated or unobligated balances that occurred in the previous fiscal year but were not recorded in the appropriate TAFS as of October 1 of the current fiscal year. Reference: OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="ProgramCode" nillable="true" type="fin:ProgramCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept that represents the amount of cost or revenue directly or indirectly traceable to programs. Reference: TFM</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="ProgramReportCategory" nillable="true" type="nc:TextType">
    	<xs:annotation>
    		<xs:documentation>A data concept that identifies a program report category that agencies use when reporting their obligations in their detailed financial information. Agencies may use this code when reporting either Category A or Category B obligations. Unlike the Apportionment Category B Program, this code is NOT subject to the Anti-Deficiency Act. Reference: OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="ReductionCode" nillable="true" type="fin:ReductionCodeType">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents the type of reduction being reported (e.g., Across The Board, Sequestration or Other) in detailed financial information. Reference: OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="ReimburseableCode" nillable="true" type="fin:ReimburseableCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept that represents whether amounts for goods, services, and joint project support are financed by offsetting collections. Reference: OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="ReportingCode" nillable="true" type="fin:ReportingCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept that indicates at the TAS level activity related to non-Federal ownership interest or statutory dedication of specifically identified revenues to designated activities. Reference: SFFAS 27</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="TASAttributes" nillable="true" type="fin:TASAttributesType">
    	<xs:annotation>
    		<xs:documentation>A data concept representing the components that comprise the Treasury Account Symbol Attributes.</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="TASStatusCode" nillable="true" type="fin:TASStatusCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept representing the budget authority for a Treasury Account Symbol. Reference: SFFAS 27</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="TASStatusTransitioningCode" nillable="true" type="fin:TASStatusTransitioningCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept representing the transition status of a Treasury Account Symbol. Reference: TFM</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="TradingPartnerAgencyValue" nillable="true" type="niem-xs:nonNegativeInteger">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents the agency identifier of the other department, agency, or establishment of the U. S. government involved in transactions with the reporting entity. Reference: FAST Book</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="TradingPartnerMainAccountCodeValue" nillable="true" type="niem-xs:nonNegativeInteger">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents the treasury main account code of the other department, agency, or establishment of the U. S. Government involved in transactions with the reporting entity. Reference: FAST Book</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="USSGL" nillable="true" type="fin:USSGLType">
    	<xs:annotation>
    		<xs:documentation>A data concept representing the US Standard General Ledger.</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="USSGLAccount" nillable="true" type="fin:USSGLAccountType">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents a USSGL account including an account number and title.</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
        <xs:element name="USSGLAccountAttributes" nillable="true" type="fin:USSGLAccountAttributesType">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents USSGL account attributes.</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="USSGLAccountNumber" nillable="true" type="niem-xs:nonNegativeInteger">
		<xs:annotation>
			<xs:documentation>A data concept representing a US Government Standard General Ledger (USSGL) account number.</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:element name="USSGLAccountTitle" nillable="true" type="nc:TextType">
		<xs:annotation>
			<xs:documentation>A data concept representing a title for a US Government Standard General Ledger (USSGL) account number.</xs:documentation>
		</xs:annotation>	
	</xs:element>
    
    <xs:element name="USSGLAttributes" nillable="true" type="fin:USSGLAttributesType">
    	<xs:annotation>
    		<xs:documentation>A data concept that represents USSGL attributes within the USSGL.</xs:documentation>
    	</xs:annotation>
    </xs:element>
    
    <xs:element name="YearOfBudgetAuthorityCode" nillable="true" type="fin:YearOfBudgetAuthorityCodeType">
        <xs:annotation>
    		<xs:documentation>A data concept that identifies whether outlays are from the new budget authority (NEW) or from budget authority carried forward from the prior year (BAL). Reference: OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    </xs:element>
</xs:schema>

