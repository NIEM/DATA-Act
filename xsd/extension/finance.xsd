<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://release.niem.gov/niem/finance/3.0/" version="1"
		   xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
		   xmlns:term="http://release.niem.gov/niem/localTerminology/3.0/" 
		   xmlns:structures="http://release.niem.gov/niem/structures/3.0/" 
		   xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" 
		   xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/3.0/"
		   xmlns:fin="http://release.niem.gov/niem/finance/3.0/"
		   xmlns:xs="http://www.w3.org/2001/XMLSchema"
		   ct:conformanceTargets="http://reference.niem.gov/niem/specification/naming-and-design-rules/3.0/#ExtensionSchemaDocument">
	<xs:annotation>
		<xs:documentation>Defines NIEM adapter types for external financial components defined by the Department of the Treasury and Office of Management and Budget in support of the DATA Act.  It references local copies of unmodified schemas from external standards in local directory tree fragments that mirror the directory structures of the canonical schema sources on the world wide web.</xs:documentation>
		<xs:appinfo>
			<term:LocalTerm term="BEA" definition="Budget Enforcement Act"/>
			<term:LocalTerm term="OMB" definition="Office of Management and Budget"/>
			<term:LocalTerm term="TAFS" definition="Treasury Appropriation Fund Symbol"/>
			<term:LocalTerm term="TAS" definition="Treasury Account Symbol"/>						
			<term:LocalTerm term="USSGL" definition="United States Standard General Ledger"/>
		</xs:appinfo>
	</xs:annotation>
	
	<xs:import schemaLocation="../niem/niem-core/3.0/niem-core.xsd" namespace="http://release.niem.gov/niem/niem-core/3.0/"/>
	<xs:import schemaLocation="../niem/structures/3.0/structures.xsd" namespace="http://release.niem.gov/niem/structures/3.0/"/>
	<xs:import schemaLocation="../niem/proxy/xsd/3.0/xs.xsd" namespace="http://release.niem.gov/niem/proxy/xsd/3.0/"/>
	
	<xs:simpleType name="AnticipatedCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type that indicates that the transaction is expected or anticipated to occur in the current fiscal year. Reference: USSGL</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>No</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Yes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AnticipatedCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates that the transaction is expected or anticipated to occur in the current fiscal year. Reference: USSGL</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:AnticipatedCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="ApportionmentCategoryCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for representing apportionment categories. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Category A - Quarterly Apportionments</xs:documentation>
				</xs:annotation>	 				
			</xs:enumeration>
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation>Category B - Apportionments other than quarterly</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Exempt from Apportionment</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ApportionmentCategoryCodeType">
		<xs:annotation>
			<xs:documentation>An data type representing apportionments by quarter, or by other specified time periods. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:ApportionmentCategoryCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="AuthorityCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type that distinguishes among the types of budgetary resources, where it is not possible to do so by the USSGL Account Number Code. Reference: OMB Circ. No. A-11, TFM 2-4200</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation>Borrowing Authority</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Contract Authority</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Advance Appropriation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Appropriation (Advance Funding) available in Prior Year</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="F">
				<xs:annotation>
					<xs:documentation>Appropriation (Advance Funding) available from Subsequent Year</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>Appropriation (excluding Advance Funding)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>Reappropriation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>Spending Authority from Offsetting Collections</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>			   
	</xs:simpleType>
	<xs:complexType name="AuthorityCodeType">
		<xs:annotation>
			<xs:documentation>A data type that distinguishes among the types of budgetary resources, where it is not possible to do so by the USSGL Account Number Code. Reference: OMB Circ. No. A-11, TFM 2-4200</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:AuthorityCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="AvailabilityTimeSimpleCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether a budgetary resource is available for new obligations in the current period, or in a subsequent period within the current fiscal year or after being reapportioned in a future fiscal year. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Available in current period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>Available in subsequent period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AvailabilityTimeCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether a budgetary resource is available for new obligations in the current period, or in a subsequent period within the current fiscal year or after being reapportioned in a future fiscal year. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:AvailabilityTimeSimpleCodeType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
		<xs:simpleType name="BEACategorySimpleCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether the Budget Enforcement Act (BEA) category is mandatory or discretionary. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Discretionary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M">
				<xs:annotation>
					<xs:documentation>Mandatory</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BEACategoryCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether the Budget Enforcement Act (BEA) category is mandatory or discretionary. Reference: OMB Circ. No. A-11</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:BEACategorySimpleCodeType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="BeginEndSimpleCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether the balance or an USSGL account/attribute combination is at the start of the fiscal year or at the end of a period. Reference: TFM</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation>Beginning Balance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Ending Balance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BeginEndCodeType">
		<xs:annotation>
			<xs:documentation>A data type that indicates whether the balance or an USSGL account/attribute combination is at the start of the fiscal year or at the end of a period. Reference: TFM</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="fin:BeginEndSimpleCodeType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<xs:simpleType name="BorrowingSourceCodeSimpleCodeType">
        <xs:annotation>
        	<xs:documentation>A data type that indicates whether borrowing took place from the public, Treasury, or a Federal financing bank. Reference: TFM, OMB Circ. No. A-11</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="F">
				<xs:annotation>
					<xs:documentation>Federal Financing Bank</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>Public</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>Treasury</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="BorrowingSouceCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether borrowing took place from the public, Treasury, or a Federal financing bank. Reference: TFM, OMB Circ. No. A-11</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:BorrowingSourceCodeSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
	<xs:simpleType name="BudgetaryImpactSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether financing resources and non-exchange revenue have an impact on the budget. Reference: TFM</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Budgetary Impact</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Non-Budgetary Impact</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BudgetaryImpactCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether financing resources and non-exchange revenue have an impact on the budget. Reference: TFM</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:BudgetaryImpactSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
    
    <xs:simpleType name="BudgetaryProprietaryCodeSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates if a USSGL account is reported on a budgetary, proprietary statement or both.  Reference: OMB Circ. No. A-11, OMB Circ. No. A-136, TFM</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Both Budgetary and Proprietary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation>Budgetary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>Proprietary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BudgetaryProprietaryCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates if a USSGL account is reported on a budgetary, proprietary statement or both.  Reference: OMB Circ. No. A-11, OMB Circ. No. A-136, TFM</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:BudgetaryProprietaryCodeSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="CustodialNoncustodialSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether reported amounts are custodial or noncustodial.  Reference: SFFAS #7, OMB Circ. No. A-136</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Non-custodial</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>Custodial</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	 <xs:complexType name="BulkFileAttributesType">
    	<xs:annotation>
    		<xs:documentation>A data type representing detailed below the 6-digit USSGL account. Agencies capture this information at the transaction level by recording transactions using USSGL 6-digit accounts plus attributes.</xs:documentation>
    	</xs:annotation>
    	<xs:complexContent>
    		<xs:extension base="structures:ObjectType">
    			<xs:sequence>
    				<xs:element ref="fin:BeginEndCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:DebitCreditCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:AuthorityCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ApportionmentCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ApportionmentCategoryBProgramValue" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:AvailabilityTimeCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:BEACategoryCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:BorrowingSourceCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:BudgetaryImpactCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:CreditCohortYearDate" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:CustodialNoncustodialCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ExchangeNonexchangeCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:FederalNonfederalCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:TradingPartnerAgencyValue" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:TradingPartnerMainAccountCodeValue" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:PriorYearAdjustmentCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ProgramCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ProgramReportCategory" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ReimburseableCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:YearOfBudgetAuthorityCode" minOccurs="0" maxOccurs="unbounded"/>
    				<xs:element ref="fin:ReductionCode" minOccurs="0" maxOccurs="unbounded"/>   				
    			</xs:sequence>
    		</xs:extension>
    	</xs:complexContent>
    </xs:complexType>
    
	
	<xs:complexType name="CustodialNoncustodialCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether reported amounts are custodial or noncustodial.  Reference: SFFAS #7, OMB Circ. No. A-136</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:CustodialNoncustodialSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
    
    <xs:simpleType name="DebitCreditSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type indicates whether the balance reported is a debit or credit.  Reference: OMB Circ. No. A-11, OMB Circ. No. A-136, TFM</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Debit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Credit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DebitCreditCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type indicates whether the balance reported is a debit or credit.  Reference: OMB Circ. No. A-11, OMB Circ. No. A-136, TFM</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:DebitCreditSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="ExchangeNonexchangeSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether the revenue, gains or losses balances being reported is exchange (X), nonexchange (T) or (E) exchange revenue with little or no associated costs. Reference: SFFAS #7, TFM</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Exchange without associated costs</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>Nonexchange</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X">
				<xs:annotation>
					<xs:documentation>Exchange</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ExchangeNonexchangeCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether the revenue, gains or losses balances being reported is exchange (X), nonexchange (T) or (E) exchange revenue with little or no associated costs. Reference: SFFAS #7, TFM</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:ExchangeNonexchangeSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="FederalNonfederalSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates the type of entity involved in transactions with the reporting entity. Reference: SFFAS #7, TFM</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Non Federal Exception</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="F">
				<xs:annotation>
					<xs:documentation>Federal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="G">
				<xs:annotation>
					<xs:documentation>General Fund Only</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Non Federal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Z">
				<xs:annotation>
					<xs:documentation>Non Reciprocating Federal Activity</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="FederalNonfederalCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates the type of entity involved in transactions with the reporting entity. Reference: SFFAS #7, TFM</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:FederalNonfederalSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>

 	<xs:simpleType name="FinancingAccountSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether the Federal account symbol is a Direct Loan Financing Account, a Guaranteed Loan Financing Account, or is not a financing account as defined by the Federal Credit Reform Act of 1990. Reference: SF 133, P and F</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Direct</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="G">
				<xs:annotation>
					<xs:documentation>Guaranteed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Nonfinancing</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="FinancingAccountCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that indicates whether the Federal account symbol is a Direct Loan Financing Account, a Guaranteed Loan Financing Account, or is not a financing account as defined by the Federal Credit Reform Act of 1990. Reference: SF 133, P and F</xs:documentation>
    	</xs:annotation>
    	<xs:simpleContent>
    		<xs:extension base="fin:FinancingAccountSimpleCodeType">
    			<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
    		</xs:extension>
    	</xs:simpleContent>
    </xs:complexType>
    
     <xs:simpleType name="FundTypeSimpleCodeType">
    	<xs:annotation>
    		<xs:documentation>A data type that represents a classification established in law that describes an OMB account's relationship to the Government, and the source of the receipts that the account is provided. Reference: FAST Book</xs:documentation>
    	</xs:annotation>
        <xs:restriction base="xs:token">
			<xs:enumeration value="CF">
				<xs:annotation>
					<xs:documentation>Clearing Account (F3500- F3885)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DF">
				<xs:annotation>
